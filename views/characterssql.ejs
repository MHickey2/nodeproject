<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="../app.css" type="text/css">
   


</head>
<body>   
    <% include ../partial/header %>
    
    <br>
    <br>

   
<h1>Hello Look at me</h1>
    
    <a href="/createsql" class="button">add a Character!</a>

         
    
        
        <div class="grid-container2">
            
        
            <% res1.forEach(function(characters) { %>

            <div class="item2">
                <div class="item">
                    <h1 class="itemtitle">Id: <%= characters.Id %></h1>
                    <h1 class="itemp">Name: <%= characters.Name %></h1>
                      <img src="/<%= characters.Image %>" class="pimage"></image>
                   
                    <h1 class="itemp">Description: <%= characters.Description %></h1>
                    <a href="/deletesql/<%= characters.Id %>" class="button">Delete Me!</a>
                    <a href="/edit/<%= characters.Id %>" class="button">Edit Me!</a> 
                    <a href="/show/<%= characters.Id %>" class="button">See Individual Character!</a>
                      
                </div>
            </div>


            <% }); %>


        </div>
        </h1>
    </div>
    
   
<br>
            <br>
            <br>
            
                <% include ../partial/footer %>
</body>

